<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="epdemo.perspective.EPDemoPerspectiveFactory"
            id="EPDemo.perspective"
            name="EPDemo透视图">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="EPDemo.category"
            name="EPDemoViewCatagory">
      </category>
      <view
            category="EPDemo.category"
            class="epdemo.views.EPDemoNavigatorViewPart"
            id="EPDemo.view"
            name="导航"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="epdemo.editors.EPDemoNormalEditor"
            default="false"
            id="EPDemo.editor"
            name="编辑器">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="EPDemo.propertyContributor"
            labelProvider="epdemo.tab.contributor.EPDemoContributorLabelProvider">
         <propertyCategory
               category="EPDemo"></propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="EPDemo.propertyTabs">
         <propertyTab
               category="EPDemo"
               id="EPDemo.propertyTab"
               label="常规">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="EPDemo.propertySections">
         <propertySection
               class="epdemo.tab.sections.EPDemoAbstractPropertySection"
               id="EPDemo.propertySection"
               tab="EPDemo.propertySection">
         </propertySection>
      </propertySections>
   </extension>


</plugin>
